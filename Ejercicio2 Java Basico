package com.example;

public class Entregable2 {

    public static void main(String[] args) {

        double precio =  precioConIva (100.45 , 18);
        System.out.println(precio);

    }
    static double precioConIva (double precioSinIva , double Iva) {
        return ((precioSinIva* Iva/100) + precioSinIva );

    }
}
